{
	"version": 2,
	"builds": [
		{
			"src": "client/package.json",
			"use": "@vercel/static-build"
		},
		{
			"src": "server/src/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/server/src/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "/client/public/index.html"
		}
	]
}
